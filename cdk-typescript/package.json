{
  "name": "cdk-typescript",
  "version": "0.1.0",
  "bin": {
    "cdk-typescript": "bin/cdk-typescript.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "lint": "eslint --ext .ts .",
    "lint:fix": "eslint --fix --ext .ts .",
    "cdk": "npx cdk",
    "prepare:func": "yarn tsc ./lib/functions/**/*.ts",
    "bootstrap:dev": "cdk bootstrap --context env=dev",
    "deploy:dev:iam": "cdk deploy management --context env=dev",
    "deploy:dev:base": "cdk deploy infra --context env=dev",
    "destroy:dev:base": "cdk destroy infra --context env=dev",
    "destroy:dev:app": "cdk destroy app --context env=dev",
    "deploy:stg": "cdk deploy --context env=stg",
    "deploy:prd": "cdk deploy --context env=prd",
    "postinstall": "npx patch-package"
  },
  "devDependencies": {
    "aws-cdk": "1.107.0",
    "@aws-cdk/assert": "1.107.0",
    "@types/jest": "^26.0.10",
    "@types/node": "10.17.27",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "eslint": "^7.19.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "prettier": "^2.2.1",
    "jest": "^26.4.2",
    "ts-jest": "^26.2.0",
    "ts-node": "^9.0.0",
    "typescript": "~3.9.7"
  },
  "dependencies": {
    "@aws-cdk/aws-applicationautoscaling": "1.107.0",
    "@aws-cdk/aws-ec2": "1.107.0",
    "@aws-cdk/aws-ecr": "1.107.0",
    "@aws-cdk/aws-ecs": "1.107.0",
    "@aws-cdk/aws-ecs-patterns": "1.107.0",
    "@aws-cdk/aws-elasticloadbalancingv2": "1.107.0",
    "@aws-cdk/aws-iam": "1.107.0",
    "@aws-cdk/aws-ssm": "1.107.0",
    "@aws-cdk/core": "1.107.0"
  }
}
